{
	"name": "living-twilight",
	"publisher": "illumincrotty",
	"displayName": "Living Twilight",
	"description": "A theme for the wee hours of the morning and night",
	"version": "1.0.2",
	"icon": "images/living-twilight.png",
	"capabilities": {
		"virtualWorkspaces": true
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/illumincrotty/VSCode-Themes"
	},
	"files": [
		"images",
		"themes"
	],
	"galleryBanner": {
		"color": "#1b1b1d",
		"theme": "dark"
	},
	"keywords": [
		"dark",
		"bright",
		"clean",
		"minimal",
		"pretty"
	],
	"engines": {
		"vscode": "^1.60.0"
	},
	"categories": [
		"Themes"
	],
	"license": "MIT",
	"scripts": {
		"build": "vsce package -o build",
		"list": "vsce ls",
		"publish": "vsce publish"
	},
	"contributes": {
		"themes": [
			{
				"label": "Living Twilight",
				"uiTheme": "vs-dark",
				"path": "./themes/living-twilight-color-theme.json"
			}
		]
	},
	"devDependencies": {
		"vsce": "^1.100.0"
	}
}
